{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "module": "CommonJS",
    "moduleResolution": "node",
    "outDir": "./dist", // Pastikan ini menunjuk ke folder 'dist'
    "noEmit": false,
    "jsx": "react"
  },
  "include": [
    "jobs/**/*.ts",      // Sertakan semua file .ts di dalam 'jobs'
    "jobs/**/*.tsx"      // Sertakan semua file .tsx di dalam 'jobs'
  ],
  "exclude": [
      "node_modules"
  ]
}